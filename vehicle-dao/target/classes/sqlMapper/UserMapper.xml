    <sql id="select_column_list">SELECT `id`,`name`,`login_name`,`code`,`pwd`,`salt`,`mobile`,`status_val`,`status_code`,`remark`,`create_at`,`create_by`,`create_name`,`update_at`,`update_by`,`update_name`,`is_del`,`is_test`</sql>
